set runtimepath=/Users/ddj/.vimperator
source! /Users/ddj/.vimperatorrc.local
set visualbell
set gui=tabs,nonavigation,noaddons,nobookmarks,nomenu
hi Bell display: none;
let mapleader = ','
map <Leader>n t<CR>
map <left> gT
map <right> gt
map = zI
map -- - zO
map <Leader>a :set invapptab<CR>
" vim: set ft=vimperator:
